"HostSettings": [
    {
      "name": "ServerHostname",
      "yaml": "SERVER_HOSTNAME",
      "type": "string",
      "default": "",
      "validate": "required"
    },
    {
      "name": "PreferredURLScheme",
      "yaml": "PREFERRED_URL_SCHEME",
      "type": "string",
      "default": "http",
      "validate": "oneof=http https"
    },
    {
      "name": "ExternalTLSTermination",
      "yaml": "EXTERNAL_TLS_TERMINATION",
      "type": "bool",
      "default": "false",
      "validate": ""
    }
  ],
  "TagExpiration": [
    {
      "name": "FeatureChangeTagExpiration",
      "yaml": "FEATURE_CHANGE_TAG_EXPIRATION",
      "type": "bool",
      "default": "false",
      "validate": ""
    },
    {
      "name": "DefaultTagExpiration",
      "yaml": "DEFAULT_TAG_EXPIRATION",
      "type": "string",
      "default": "2w",
      "validate": "required"
    },
    {
      "name": "TagExpirationOptions",
      "yaml": "TAG_EXPIRATION_OPTIONS",
      "type": "[]interface{}",
      "default": "[\"0s\", \"1d\", \"1w\", \"2w\", \"4w\"]",
      "validate": "required"
    }
  ],
  "UserVisibleSettings": [
    {
      "name": "RegistryTitle",
      "yaml": "REGISTRY_TITLE",
      "type": "string",
      "default": "Project Quay",
      "validate": "omitempty"
    },
    {
      "name": "RegistryTitleShort",
      "yaml": "REGISTRY_TITLE_SHORT",
      "type": "string",
      "default": "Project Quay",
      "validate": "omitempty"
    },
    {
      "name": "SearchResultsPerPage",
      "yaml": "SEARCH_RESULTS_PER_PAGE",
      "type": "int",
      "default": "10",
      "validate": "omitempty"
    },
    {
      "name": "SearchMaxResultPageCount",
      "yaml": "SEARCH_MAX_RESULT_PAGE_COUNT",
      "type": "int",
      "default": "10",
      "validate": "omitempty"
    },
    {
      "name": "ContactInfo",
      "yaml": "CONTACT_INFO",
      "type": "[]interface{}",
      "default": "[]",
      "validate": "omitempty"
    },
    {
      "name": "AvatarKind",
      "yaml": "AVATAR_KIND",
      "type": "string",
      "default": "local",
      "validate": "omitempty,oneof=local gravatar"
    },
    {
      "name": "Branding",
      "yaml": "BRANDING",
      "type": "interface{}",
      "default": "",
      "validate": "omitempty",
      "properties": [
        {
          "name": "Logo",
          "yaml": "logo",
          "type": "string",
          "default": "",
          "validate": "omitempty,url"
        },
        {
          "name": "FooterIMG",
          "yaml": "footer_img",
          "type": "string",
          "default": "",
          "validate": "omitempty,url"
        },
        {
          "name": "FooterURL",
          "yaml": "footer_url",
          "type": "string",
          "default": "",
          "validate": "omitempty,url"
        }
      ]
    }
  ],