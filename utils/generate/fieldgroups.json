{
  "packageName": "fieldgroups",
  "fieldGroups": [
    {
      "fieldGroupName": "TagExpiration",
      "fields": [
        {
          "fieldName": "FeatureChangeTagExpiration",
          "fieldYAML": "FEATURE_CHANGE_TAG_EXPIRATION",
          "fieldType": "bool",
          "fieldDefault": "false",
          "fieldValidate": ""
